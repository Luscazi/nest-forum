{
  "extends": "@rocketseat/eslint-config/node",
  "rules": {
    "typescript-eslint/no-empty-function": "off",
    "no-useless-constructor": "off",
    "semi": ["error", "never"],
    "prettier/prettier": ["error", {
      "endOfLine": "auto",
      "printWidth": 80,
      "tabWidth": 2,
      "singleQuote": true,
      "trailingComma": "all",
      "arrowParens": "always",
      "semi": false
    }]
  }
}